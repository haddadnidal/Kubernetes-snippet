".source.yaml":
  "Create a Deployment":
    "prefix": "Deployment"
    "body" : """
apiVersion: apps/v1
kind: Deployment
metadata:
  name: ${1:<myapp>}
  labels:
    app: ${1:<myapp>}
spec:
  replicas: 1
  selector:
    matchLabels:
      app: ${1:<myapp>}
  template:
    metadata:
      labels:
        app: ${1:<myapp>}
    spec:
      containers:
      - name: ${1:<myapp>}
        image: ${2:<Image>}
        ports:
        - containerPort: ${3:<Port>}
"""
